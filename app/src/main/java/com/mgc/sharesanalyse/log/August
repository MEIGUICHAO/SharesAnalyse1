select * from AA_REV_OC_OO_30 where _ID in (select max(_ID) from AA_REV_OC_OO_30 group by CODE,DATE)
SELECT * FROM AA_REV_OC_OO_30 WHERE _ID in (select max(_ID) from AA_REV_OC_OO_30 group by CODE,DATE) AND OC10 >=0 AND OC10<20 AND ( CODE = 12)
SELECT * FROM AA_REV_OC_OO_30 WHERE _ID in (select max(_ID) from AA_REV_OC_OO_30 group by CODE,DATE) AND  OC3 >=0 AND OC3<20 AND ( CODE =12 AND DATE = '20201021') OR  (CODE =12 AND DATE = '20201022') OR  (CODE =12 AND DATE = '20201019') OR  (CODE =12 AND DATE = '20201021') OR  (CODE =12 AND DATE = '20201022')
SELECT * FROM AA_REV_OC_OO_30 WHERE   OC3 >=0 AND OC3<20 AND (( CODE =12 AND DATE = '20201021') OR  (CODE =12 AND DATE = '20201022') OR  (CODE =12 AND DATE = '20201019') OR  (CODE =12 AND DATE = '20201021') OR  (CODE =12 AND DATE = '20201022'))
SELECT * FROM AA_REV_OC_OO_30 WHERE   ( CODE =12 AND DATE = '20201021') OR  (CODE =12 AND DATE = '20201022') OR  (CODE =12 AND DATE = '20201019') OR  (CODE =12 AND DATE = '20201021') OR  (CODE =12 AND DATE = '20201022')

